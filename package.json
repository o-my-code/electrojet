{
  "name": "electrojet",
  "version": "0.0.1",
  "description": "The no hassle Electron boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "run-p webpack-serve electron-wait",
    "webpack-dev": "webpack --mode development",
    "webpack-build": "webpack --mode production",
    "webpack-serve": "webpack-dev-server --mode development --port 4567",
    "electron": "NODE_ENV=DEV electron .",
    "wait": "wait-on http://localhost:4567/",
    "electron-wait": "run-s wait electron",
    "electron-package-all": "electron-packager . electrojet --platform=all",
    "electron-package-mac": "electron-packager . electrojet --platform=darwin",
    "electron-package-linux": "electron-packager . electrojet --platform=linux",
    "electron-package-win": "electron-packager . electrojet --platform=win32",
    "electron-package-mas": "electron-packager . electrojet --platform=mas",
    "build": "run-s webpack-build electron-package-all"
  },
  "author": "Vinay Chandran",
  "license": "ISC",
  "dependencies": {
    "electron": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chokidar": "^2.0.4",
    "css-loader": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.0.1",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "wait-on": "^3.2.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
